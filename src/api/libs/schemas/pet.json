{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "A pet schema",
  "type": "object",
  "properties": {
    "id": {
      "type": "number"
    },
    "category": {
      "type": "object",
      "properties": {
        "id": {
          "type": "number"
        },
        "name": {
          "type": "string"
        }
      }
    },
    "name": {
      "type": "string"
    },
    "photoUrls": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "name": {
            "type": "string"
          }
        }
      }
    },
    "status": {
      "type": "string",
      "enum": ["pending", "available", "sold"]
    }
  }
}
